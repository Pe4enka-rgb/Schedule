<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ev="clr-namespace:Schedule.View.EntityView"
    xmlns:evm="clr-namespace:Schedule.ViewModels.EntityViewModels"
    xmlns:view="clr-namespace:Schedule.View"
    xmlns:vm="clr-namespace:Schedule.ViewModels">

    <DataTemplate DataType="{x:Type evm:SubjectViewModel}">
        <ev:SubjectView DataContext="{Binding}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type evm:BellViewModel}">
        <ev:BellView DataContext="{Binding}" />
    </DataTemplate>


    <DataTemplate DataType="{x:Type evm:SchoolClassViewModel}">
        <ev:SchoolClassView DataContext="{Binding}" />
    </DataTemplate>


    <DataTemplate DataType="{x:Type evm:GradeViewModel}">
        <ev:GradeView DataContext="{Binding}" />
    </DataTemplate>


</ResourceDictionary>